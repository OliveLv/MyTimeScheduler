html
	head
		title Calendar
		// set encode to utf-8,deal with chinese garbled
		meta(http-equiv="Content-Type",content="text/html; charset=utf-8")
		link(rel='stylesheet',type='text/css',href='css/jquery-ui.css')
		link(rel='stylesheet',type='text/css',href='css/jquery.ui.theme.css')
		link(rel='stylesheet',type='text/css',href='css/timelineScheduler.css')
		link(rel='stylesheet',type='text/css',href='css/timelineScheduler.styling.css')
		link(rel='stylesheet',type='text/css',href='css/calendar.css')
		script(src='js/moment.min.js')
		script(src='js/jquery-1.9.1.min.js')
		script(src='js/jquery-ui-1.10.2.min.js')
		script(src='js/timelineScheduler.js')
		script(src='js/calendar.js')
	body
		script.
			Calendar.Sections=!{JSON.stringify(sections)};
			var items=!{JSON.stringify(items)};
			for(var i=0;i<items.length;i++){
				items[i].end=moment(items[i].end).add('hours',2);
				items[i].start=moment(items[i].start);
				for(var j=0;j<items[i].events.length;j++){
					items[i].events[j].at=moment(items[i].events[j].at);
				}
			}
			Calendar.Items=items;
			console.log(Calendar);

		div.calendar
		div.realtime-info
		

